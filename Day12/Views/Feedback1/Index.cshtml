@model IEnumerable<Day12.Models.Feedback1>

@{
    ViewData["Title"] = "Feedback Form";
}


<hr/>
<div class="card-body text-center bg-white border-black mt-2"><h1>Feedback Form List </h1></div>
<hr/>
<div class="row justify-content-center">
<div class="col-md-10 mt-4">
<div class="card shadow border-secondary">
<div class="card-body">
<table class="table">
    <thead>
        
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mobile_Number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Feedback)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mobile_Number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Feedback)
            </td>
            <td>
                @*                 <a asp-action="Edit" asp-route-id="@item.Id">Edit</a>*@              
                <pre><a class="btn btn-primary" asp-action="Details" asp-route-id="@item.Id">Details</a> <a class="btn btn-danger" asp-action="Delete" asp-route-id="@item.Id">Delete</a> </pre>
             </td>
        </tr>
}
    </tbody>
</table>
</div>
</div>
</div>
</div>
